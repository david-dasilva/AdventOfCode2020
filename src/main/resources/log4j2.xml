<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
<Appenders>
    <Console name="Console" target="SYSTEM_OUT">
        <PatternLayout pattern="%d{HH:mm:ss.SSS} %highlight{%-5level} %m%n"/>
    </Console>
</Appenders>
<Loggers>
    <Logger name="net.daviddasilva" level="TRACE"/>
    <Root level="error">
        <AppenderRef ref="Console"/>
    </Root>
</Loggers>
</Configuration>